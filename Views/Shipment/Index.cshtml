                    <input class="btn btn-default form-control" type="button" id="btnValidateUPSAccount" value="Validate Account" onclick="validateUPSAccount();" />
                </div>
                <div class="form-group">

                    <label for="shipperNumber">
                        Shipper Number:
                    </label>

                    <input class="form-control" id="shipperNumber" type="text" value="@ViewBag.ShipperNumber" readonly="readonly" />


                </div>
                <div class="form-group">
                    <label for="ddlBillTo"> Bill To:</label>

                    <div class="form-control">
                        @Html.DropDownList("billToOption", (IEnumerable<SelectListItem>)ViewBag.BillToOptionlist, "-- Select --", new { id = "ddlBillTo" })
                        <div style="display: none;">
                            <input id="txtbillTo" value="@ViewBag.ShipperNumber" type="text" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label for="txtUPSAccountNumber">UPS Account:</label>
                    <input class="form-control" id="txtUPSAccountNumber" type="text" value="@ViewBag.ShipperNumber" />
                </div>




                <div class="form-group">

                    <label for="selectrequestoptionid">
                        Request option:
                    </label>

                    <div class="form-control">
                        @Html.DropDownList("requestoption", (IEnumerable<SelectListItem>)ViewBag.RequestOptionlist, "Select Request Option", new { id = "selectrequestoptionid" })
