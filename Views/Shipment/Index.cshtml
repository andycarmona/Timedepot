        function SetDefaultLogCheckBox(szckCriteria) {

            jQuery("#ckcustLog").prop("checked", false);
            switch (szckCriteria) {
            case "invoice":
                jQuery("#ckcustLog").prop("checked", true);
                break;
            case "shipmentrecord":
