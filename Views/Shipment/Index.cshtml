                            <input id="customertype" type="text" />
                        </div>
                    </div>

                </div>

                <div class="form-group">

                    <label for="selectupsServiceTypeid">
                        UPS Service Type:
                    </label>
                    <div class="form-control">
                        @Html.DropDownList("upsServiceType", (IEnumerable<SelectListItem>)ViewBag.UpsServiceTypelist, "Select UPS Service Type", new { id = "selectupsServiceTypeid" })
                        <div style="display: none;">
                            <input id="upsServiceType" type="text" />
                        </div>
                    </div>
                </div>

                <div class="form-group">

                    <label for="selectpacakePackTypeid">
                        Package pack type:
                    </label>
