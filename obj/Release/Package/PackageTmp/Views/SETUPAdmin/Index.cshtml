@using PagedList.Mvc;   @*//import this so we get our HTML Helper*@
@using PagedList;       @*//import this so we can cast our list to IPagedList (only necessary because ViewBag is dynamic)*@
@model IEnumerable<TimelyDepotMVC.Models.Admin.SETUP>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>SETUP</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>SetupID</th>
        <th>
            @Html.DisplayNameFor(model => model.Run_charge)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SetupCost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Discount_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Min)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dial)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SetupID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SetupID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SetupID })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SetupID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Run_charge)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SetupCost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Discount_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Min)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dial)
        </td>
    </tr>
}
    <tr>
        <td colspan="8">
            <!-- output a paging control that lets the user navigation to the previous page, next page, etc -->
            @Html.PagedListPager((IPagedList)ViewBag.OnePageOfData, page => Url.Action("Index", new { page }))

        </td>
    </tr>

</table>
